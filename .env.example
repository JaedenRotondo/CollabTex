# Database connection string
DATABASE_URL=file:local.db

# Signaling server configuration
# URL of your signaling server (deployed on Render)
VITE_SIGNALING_URL=wss://your-signaling-server.onrender.com

# API key for signaling server authentication
# This should match the SIGNALING_API_KEY on your signaling server
VITE_SIGNALING_API_KEY=your-secure-api-key-here

# LaTeX compilation server (optional)
LATEX_SERVER_URL=https://your-latex-server.onrender.com