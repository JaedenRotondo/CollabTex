# Port for the signaling server
PORT=4444

# Comma-separated list of allowed origins
# Use * to allow all origins (not recommended for production)
ALLOWED_ORIGINS=https://collabtex.com,https://www.collabtex.com

# API key for authentication (generate a secure random string)
SIGNALING_API_KEY=your-secure-api-key-here

# Node environment
NODE_ENV=production